<%= render 'shared/menu' %>
<h1>Listing <%= @objects.first.class.model_name.human.pluralize %></h1>

<table>

<% @objects.each do |student| %>
  <tr>
    <td><%= link_to [student.last_name, student.first_name].join(', '), action: 'show', id: student.id %></td>
    <td><%= link_to 'Edit', action: 'edit', id: student.id %></td>
    <td><%= link_to 'Destroy', {action: 'destroy', id: student.id}, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to "New #{@objects.first.class.model_name.human}", action: 'new' %>

